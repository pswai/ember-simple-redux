{
  "name": "ember-simple-redux",
  "version": "0.4.0",
  "description": "React-redux-compatible way to use Redux in Ember applications.",
  "keywords": [
    "ember-addon",
    "redux",
    "react",
    "react-redux"
  ],
  "repository": "https://github.com/pswai/ember-simple-redux",
  "license": "MIT",
  "author": "PSWai <parksoon_my@hotmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "+(*.{json,css,md}|.prettierrc|.watchmanconfig)": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "ember-auto-import": "^1.2.21",
    "ember-cli-babel": "^7.1.2",
    "react": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "semver": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@ember/jquery": "^0.6.0",
    "@ember/optional-features": "^0.7.0",
    "broccoli-asset-rev": "^3.0.0",
    "chai": "^4.2.0",
    "ember-cli": "~3.8.1",
    "ember-cli-chai": "^0.5.0",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-htmlbars": "^3.0.1",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-mocha": "^0.14.0",
    "ember-resolver": "^5.1.3",
    "ember-sinon": "^3.1.0",
    "ember-source": "~3.8.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.1.0",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-ember": "^7.6.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "2.6.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "loader.js": "^4.7.0",
    "prettier": "^1.16.4",
    "sinon": "^7.3.0",
    "sinon-chai": "^3.3.0"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
